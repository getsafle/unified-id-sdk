{
  "name": "@unifiedid/sdk",
  "version": "1.0.0",
  "description": "Functional programming SDK for UnifiedID system - Multi-chain identity resolution and management",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "dev": "babel src -d lib --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "docs": "jsdoc -c jsdoc.json",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "unifiedid",
    "identity",
    "blockchain",
    "multi-chain",
    "ethereum",
    "polygon",
    "arbitrum",
    "base",
    "optimism",
    "bsc",
    "avalanche",
    "sepolia",
    "functional-programming",
    "javascript",
    "sdk"
  ],
  "author": "kunalmkv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unifiedid/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/unifiedid/sdk/issues"
  },
  "homepage": "https://github.com/unifiedid/sdk#readme",
  "dependencies": {
    "ethers": "^6.8.1",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "jsdoc": "^4.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
} 